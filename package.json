{
  "name": "statoscope-tech",
  "private": true,
  "scripts": {
    "start": "static public -a 0.0.0.0 -p 80",
    "build": "webpack && cp src/statoscope.png src/favicon.ico public/ && statoscope validate --config=./statoscope.config.demo.js --input public/demo-stats.json || true",
    "predev": "npm run build",
    "dev": "webpack serve",
    "prebuild:prod": "rm -rf public/*",
    "build:prod": "NODE_ENV=production npm run build",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts --cache",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "statoscope:validate": "statoscope validate --input  public/demo-stats.json",
    "statoscope:validate-diff": "statoscope validate --input input.json --reference reference.json"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@statoscope/cli": "^5.20.0",
    "@statoscope/stats-validator-plugin-webpack": "^5.20.0",
    "@statoscope/webpack-ui": "5.20.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mustache": "^4.2.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@discoveryjs/json-ext": "^0.5.3",
    "@statoscope/webpack-plugin": "5.20.0",
    "@wdxlab/events": "^1.1.0",
    "node-static": "^0.7.11"
  }
}
